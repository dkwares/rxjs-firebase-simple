{
  "name": "rxjs-firebase-simple",
  "version": "1.0.1",
  "description": "A simple RxJS interface to firebase",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc",
    "test": "karma start",
    "test:dev": "karma start karma.dev.conf.js",
    "docs": "rm -rf documentation/ && typedoc --out documentation/ ./src/index.ts --readme none --mode file --theme markdown --module commonjs  --excludePrivate --excludeExternals --mdFlavour github --mdHideSources"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkwares/rxjs-firebase-simple.git"
  },
  "keywords": [
    "rxjs",
    "rx",
    "firebase",
    "google",
    "kris rivera",
    "kris",
    "rivera"
  ],
  "author": "Kris Rivera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dkwares/rxjs-firebase-simple/issues"
  },
  "homepage": "https://github.com/dkwares/rxjs-firebase-simple#readme",
  "peerDependencies": {
    "firebase": "^4.9.0",
    "rxjs": "^5.5.2"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "firebase": "^4.9.0",
    "jasmine": "^3.0.0",
    "jasmine-core": "^2.99.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "^1.1.1",
    "karma-typescript": "^3.0.12",
    "karma-typescript-es6-transform": "^1.0.3",
    "rxjs": "^5.5.2",
    "typedoc": "^0.10.0",
    "typedoc-plugin-markdown": "^1.0.14",
    "typescript": "^2.7.2"
  },
  "files": [
    "dist/",
    "src/"
  ]
}
